{
  "name": "horat1us/yii2-base",
  "description": "Some extensions for Yii2 Framework",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "^2.0.13.1",
    "nesbot/carbon": "^1.22"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "fzaninotto/faker": "^1.7"
  },
  "keywords": [
    "yii2",
    "extension",
    "widget",
    "horat1us",
    "base"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Horat1us\\Yii\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Horat1us\\Yii\\Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "platform": {
      "php": "7.1"
    }
  }
}
